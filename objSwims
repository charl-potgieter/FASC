var objSwims = function(){

  //-------------------------------------------------------------------------------------------------------------
  //  Sundry class properties / variables
  //-------------------------------------------------------------------------------------------------------------

  var self = this;
  self.arrayOfSwims = []    // Stores swimmers in a simple array to allow for easier iteration



  //-------------------------------------------------------------------------------------------------------------
  // adds a new swimmer objSwimmer to the group
  //-------------------------------------------------------------------------------------------------------------
  
  self.addSwim = function(swim){
    self.arrayOfSwims.push(swim)
  };


};
